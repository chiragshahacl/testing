{
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix . --ext ts --ext tsx --ext js",
      "prettier --write ."
    ],
    "*.ts": [
      "eslint --fix . --ext ts --ext tsx --ext js",
      "prettier --write ."
    ],
    "*.tsx": [
      "eslint --fix . --ext ts --ext tsx --ext js",
      "prettier --write ."
    ]
  },
  "scripts": {
    "copyWasm": "copy-files-from-to --config copy-files-from-to.json",
    "dev": "npm run copyWasm && next dev",
    "build": "npm run copyWasm && next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint --fix src --ext ts --ext tsx --ext js",
    "check-format": "prettier --check .",
    "check-lint": "eslint src --ext ts --ext tsx --ext js",
    "test": "jest --silent",
    "unit-tests": "jest --coverage --silent",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "npx concurrently -k -s first -n 'SB,TEST' -c 'magenta,blue' \"npx http-server storybook-static --port 6006\" \"npx --yes wait-on http://127.0.0.1:6006 -t 60000 && test-storybook --coverage --url http://127.0.0.1:6006\" && npx nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/material": "5.15.14",
    "@mui/x-date-pickers": "7.21.0",
    "@tanstack/react-query": "4.35.0",
    "axios": "1.7.4",
    "ffi-rs": "^1.0.78",
    "formik": "^2.2.9",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "moment-timezone": "0.5.43",
    "next": "13.5.7",
    "os": "^0.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "15.0.2",
    "scichart": "3.1.329",
    "uuid": "9.0.0",
    "yup": "1.0.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@next/eslint-plugin-next": "^13.5.3",
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/blocks": "^7.5.2",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/react-query-devtools": "^4.35.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/next": "^9.0.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-jest": "28.1.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "concurrently": "^8.2.1",
    "copy-files-from-to": "^3.9.1",
    "copy-webpack-plugin": "^12.0.2",
    "esbuild": "^0.21.4",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-mui-path-imports": "^0.0.15",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "http-server": "^14.1.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.1",
    "msw": "^1.3.1",
    "msw-storybook-addon": "^1.10.0",
    "playwright": "^1.39.0",
    "prettier": "2.8.0",
    "prettier-eslint": "^15.0.1",
    "storybook": "^7.5.2",
    "typescript": "5.0.4",
    "wait-on": "^7.0.1",
    "webpack": "5.94.0"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
